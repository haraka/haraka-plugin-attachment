{
  "author": "Haraka Team <harakamail@gmail.com>",
  "name": "haraka-plugin-attachment",
  "license": "MIT",
  "description": "A message attachment scanning plugin for Haraka",
  "version": "1.0.0",
  "homepage": "http://haraka.github.io",
  "repository": {
    "type": "git",
    "url": "git@github.com:haraka/haraka-plugin-attachment.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">= 0.10.42"
  },
  "dependencies": {
    "tmp": "0.0.31"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "camelcase": [
        0,
        {
          "properties": "never"
        }
      ],
      "dot-notation": 2,
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "one-var": [
        2,
        "never"
      ],
      "no-trailing-spaces": [
        2,
        {
          "skipBlankLines": false
        }
      ],
      "keyword-spacing": [
        2,
        {
          "overrides": {}
        }
      ]
    }
  },
  "optionalDependencies": {},
  "devDependencies": {
    "eslint": ">=2.0.0",
    "grunt": "*",
    "grunt-contrib-clean": "*",
    "grunt-eslint": "*",
    "grunt-version-check": "*",
    "haraka-config": "*",
    "haraka-test-fixtures": "*"
  },
  "bugs": {
    "mail": "helpme@gmail.com",
    "url": "https://github.com/haraka/haraka-plugin-attachment/issues"
  }
}
